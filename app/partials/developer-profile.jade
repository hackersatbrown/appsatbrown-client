div(ng-controller='DeveloperProfileCtrl')
  p {{user.name}}
  p {{user.email}}
  p Apps
  div
    ul.unstyled
      li(ng-repeat='app in user.apps')
        p 
        a(href='#/apps/{{app.id}}')
          label(class='checkbox inline')
            {{app.name}}
        p
        a(href='#/developer/{{user.id}}/{{app.id}}')
          label(class='checkbox inline')
            p edit above
      li
        p
        a(href='#/developer/{{user.id}}/new')
          label(class='checkbox inline')
            p create new